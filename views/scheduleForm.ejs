<body>
    <h1 class="display-3">New Schedules</h1>
    <form action="/filter">
        <h1>term</h1>
        <select name="Term" id="selectTerm">
            <option value="202210">Fall 2021-22</option>
            <option value="202220">Spring 2021-2022</option>
            <option value="202230">Summer 2021-2022</option>
        </select>

        <h1>CRNS</h1>
        <div id="setCRNs">
        </div>
        <button id='addCRN'>Add CRN</button>

        <h1>Courses</h1>
        <div id="courses">
            <input type="text" name="sections[]" placeholder="MATH201" value="MATH201"> <br>
            <input type="text" name="sections[]" placeholder="PHYS210" value="PHYS210"> <br>
            <input type="text" name="sections[]" placeholder="EECE230" value="EECE230"> <br>
            <input type="text" name="sections[]" placeholder="BIOL201" value="BIOL201"> <br>
        </div>

        <button>Submit</button>
    </form>
</body>
